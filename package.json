{
  "name": "vue-nouns-blockie",
  "version": "1.0.13",
  "description": "Vue package that generates custom nouns avatar for web3 addresses and strings",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly --declaration",
