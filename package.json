    "@types/node": "^20.11.20",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/compiler-sfc": "^3.4.19",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vue": "3.4.19",
    "vue-tsc": "^1.8.27"
  },
  "type": "module",
  "dependencies": {
