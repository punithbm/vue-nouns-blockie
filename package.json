{
  "name": "vue-nouns-blockie",
  "version": "1.0.9",
  "description": "Vue package that generates custom nouns avatar for web3 addresses and strings",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly --declaration",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["vue", "nouns", "noun", "avatar", "nuxt"],
  "author": "punithbm",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/compiler-sfc": "^3.4.19",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vue": "3.4.19",
    "vue-tsc": "^1.8.27"
  },
  "type": "module",
  "dependencies": {
    "@nouns/assets": "^0.5.0",
    "@nouns/sdk": "^0.4.0",
    "vite-plugin-dts": "^3.7.3",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "peerDependencies": {
    "vue": "^3.4.19"
  }
}
